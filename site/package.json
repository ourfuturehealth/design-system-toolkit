{
  "name": "ofh-design-system-documentation",
  "private": true,
  "scripts": {
    "predocs:start": "npm run build --prefix ../ && mkdir -p dist",
    "docs:start": "concurrently 'npm:docs:watch:css' 'npm:docs:watch:js' 'npm:docs:serve'",
    "docs:watch:css": "npm run docs:build:css && npm run docs:build:css -- -w",
    "docs:watch:js": "npm run docs:build:js -- -w",
    "docs:build": "npm run docs:build:css && npm run docs:build:js",
    "docs:build:css": "node-sass --recursive --output-style compressed styles/ -o dist/css/",
    "docs:build:js": "webpack",
    "docs:serve": "npx @11ty/eleventy --serve",
    "docs:clean": "rm -rf dist/"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "glob": "^8.0.3",
    "highlight.js": "^11.6.0",
    "iframe-resizer": "^4.3.2",
    "js-beautify": "^1.14.6",
    "node-sass": "^7.0.1",
    "nunjucks": "^3.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
