{
  "name": "nhsuk-frontend",
  "version": "0.0.5",
  "description": "NHS.UK front-end library and design system",
  "sasslintConfig": "sass-lint.yml",
  "scripts": {
    "postinstall": "npm run build",
    "build": "gulp build",
    "start": "node app.js",
    "prewatch": "gulp build",
    "watch": "concurrently \"gulp\" \"nodemon app.js -q\"",
    "test": "npm run lint-css",
    "lint-css": "./node_modules/.bin/sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q"
  },
  "dependencies": {
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "express-flash": "0.0.2",
    "express-session": "^1.2.1",
    "express-validator": "^5.1.2",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "nunjucks": "^3.1.2",
    "sass-mq": "^4.0.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^3.6.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^7.0.1",
    "gulp-clean-css": "^3.9.4",
    "gulp-rename": "^1.3.0",
    "gulp-sass": "^4.0.1",
    "gulp-util": "^3.0.8",
    "nodemon": "^1.17.5",
    "sass-lint": "^1.12.1"
  },
  "engines": {
    "node": "8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsuk/frontend.git"
  },
  "bugs": {
    "url": "https://github.com/nhsuk/frontend/issues"
  },
  "license": "MIT"
}
