{
  "name": "nhsuk-frontend",
  "version": "0.0.5",
  "description": "NHS.UK front-end library and design system",
  "sasslintConfig": "sass-lint.yml",
  "scripts": {
    "postinstall": "npm run build",
    "build": "gulp build",
    "start": "node app.js",
    "prewatch": "gulp build",
    "watch": "concurrently \"gulp watch\" \"nodemon app.js -q\"",
    "test": "npm run lint-css",
    "lint-js": "./node_modules/.bin/eslint assets/js/*.js",
    "lint-css": "./node_modules/.bin/sass-lint -c .sass-lint.yml 'src/**/*.scss' -v -q"
  },
  "dependencies": {
    "@material/tabs": "^0.36.0",
    "body-parser": "^1.15.1",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "express-flash": "0.0.2",
    "express-session": "^1.2.1",
    "express-validator": "^5.1.2",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cssnano": "^2.1.3",
    "gulp-csso": "^3.0.1",
    "gulp-if": "^2.0.0",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.0",
    "method-override": "^2.3.5",
    "morgan": "^1.7.0",
    "node-sass": "^4.9.0",
    "nunjucks": "^3.1.2",
    "reload": "^2.2.2",
    "run-sequence": "^2.2.1",
    "sass-lint": "^1.12.1",
    "sass-mq": "^4.0.2",
    "vinyl-buffer": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-nhsuk": "^0.15.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-mocha": "^5.0.0",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-cssnano": "^2.1.3",
    "nodemon": "^1.17.3",
    "npm-check-updates": "^2.14.1",
    "run-sequence": "^2.2.1",
    "sass-lint": "^1.12.1"
  },
  "engines": {
    "node": "8.11.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhsuk/frontend.git"
