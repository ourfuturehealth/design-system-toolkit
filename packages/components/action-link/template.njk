<div class="nhsuk-action-link">
  <a href="{%- if params.href %}{{ params.href }}{% else %}#{% endif -%}"
     class="nhsuk-action-link__link{% if params.classes %} {{ params.classes }}{% endif %}"
     {%- for attribute, value in params.attributes %} {{attribute}}="{{ value }}"{% endfor %}>
    {% include 'assets/icons/icon-arrow-right-circle.svg' %}
    <span class="nhsuk-action-link__text">{{ params.text }}</span>
  </a>
</div>
