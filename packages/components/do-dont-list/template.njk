<div class="nhsuk-c-do-dont-list">
  <h3 class="nhsuk-c-do-dont-list__tab-heading" id="{{title}}">{{title}}</h3>
  {% if type == 'tick' %}
    <ul class="nhsuk-list nhsuk-list--check">
      {% for data in items %}
      <li>
      {% include 'assets/icons/icon-tick.svg' %}
      {{ data.item | safe}}
      </li>
      {% endfor %}
    </ul>
  {% elseif type == 'cross' %}
    <ul class="nhsuk-list nhsuk-list--cross">
    {% for data in items %}
      <li>
      {% include 'assets/icons/icon-cross.svg' %}
      <span>Don't</span> {{ data.item | safe}}
      </li>
    {% endfor %}
    </ul>
  {% endif %}
</div>
