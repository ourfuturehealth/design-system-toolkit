<nav id="nhsuk-a-z-nav" class="nhsuk-a-z-nav {% if params.class %}{{ params.class }}{% endif %} {%- if params.classes %} {{ params.classes }}{% endif %}" role="navigation" aria-label="A-Z Navigation">
  <ul class="nhsuk-a-z-nav__list">
    {% for item in params.items %}
      <li class="nhsuk-a-z-nav__list-item">
        {% if item.disable %}
          <p class="nhsuk-a-z-nav__list-item-link--disabled">{{ item.label }}</p>
          {% else %}
          <a href="#{{ item.label }}" class="nhsuk-a-z-nav__list-item-link">{{ item.label }}</a>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</nav>
