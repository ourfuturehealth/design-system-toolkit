<nav id="nhsuk-nav-a-z" class="nhsuk-nav-a-z {% if params.class %}{{ params.class }}{% endif %} {%- if params.classes %} {{ params.classes }}{% endif %}" role="navigation" aria-label="A-Z Navigation">
  <ul class="nhsuk-nav-a-z__list">
    {% for item in params.items %}
      <li class="nhsuk-nav-a-z__item">
        {% if item.disable %}
          <p class="nhsuk-nav-a-z__link--disabled">{{ item.label }}</p>
          {% else %}
          <a href="#{{ item.label }}" class="nhsuk-nav-a-z__link">{{ item.label }}</a>
        {% endif %}
      </li>
    {% endfor %}
  </ul>
</nav>
