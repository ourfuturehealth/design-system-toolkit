<footer role="contentinfo">
  <div class="nhsuk-footer">
    <div class="nhsuk-width-container">
      <ul class="nhsuk-footer__list">
        <li class="nhsuk-footer__logo nhsuk-footer__list-item">
          <a href="/" class="global-footer__link">
            {% include 'assets/logos/nhs-logo.svg' %}
            <span class="visually-hidden">NHS homepage</span>
          </a>
        </li>
        {% for item in primaryLinks %}
        <li class="nhsuk-footer__list-item"><a class="nhsuk-footer__list-item-link" href="{{item.URL}}">{{item.label}}</a></li>
        {% endfor %}
      </ul>
      <ul class="nhsuk-footer__list">
        {% for item in secondaryLinks %}
        <li class="nhsuk-footer__list-item"><a class="nhsuk-footer__list-item-link" href="{{item.URL}}">{{item.label}}</a></li>
        {% endfor %}
        <li class="nhsuk-footer__list-item">&copy; Crown Copyright</li>
      </ul>
    </div>
  </div>
</footer>
